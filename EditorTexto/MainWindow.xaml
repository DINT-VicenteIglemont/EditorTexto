<Window x:Class="EditorTexto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EditorTexto"
        mc:Ignorable="d"
        Title="Docs" Height="450" Width="800" ResizeMode="NoResize" SizeToContent="WidthAndHeight">
    
    <StackPanel>
        
        <Button x:Name="NuevoDocumentoButton" Content="Nuevo Documento" Margin="5" Click="NuevoDocumentoButton_Click"/>
        <Button x:Name="ConfiguracionButton" Content="Configuracion" Margin="5" Click="ConfiguracionButton_Click"/>
        
    </StackPanel>
    
</Window>
